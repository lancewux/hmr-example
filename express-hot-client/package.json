    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-plugin-ignore-html-and-css-imports": "0.0.2",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "clean-webpack-plugin": "^0.1.19",
    "cross-env": "^5.2.0",
